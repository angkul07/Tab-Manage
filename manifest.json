{
  "manifest_version": 3,
  "name": "Tab Folder Manager",
  "version": "1.0",
  "description": "Organize your tabs into folders and manage them easily",
  "permissions": [
    "tabs",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icons/icon48.png",
  }
}
